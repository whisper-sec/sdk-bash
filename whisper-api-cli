print_searchLocation_help() {
    echo ""
    echo -e "${BOLD}${WHITE}searchLocation - Search Geolocation Database by Field${OFF}${BLUE}(AUTH - )${OFF}" | paste -sd' ' - | fold -sw 80 | sed '2,$s/^/    /'
